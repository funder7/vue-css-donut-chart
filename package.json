{
  "name": "vue-css-donut-chart",
  "version": "1.4.0",
  "description": "Lightweight Vue component for drawing pure CSS donut charts",
  "author": "dumptyd <dumptyd2.0@gmail.com>",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "yarn clear:babel-cache && MODE=site vue-cli-service build --modern --dest docs",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "build-lib": "yarn clear:babel-cache && vue-cli-service build --target lib --name vcdonut src/index.js",
    "clear:babel-cache": "rm -rf ./node_modules/.cache/babel-loader/*",
    "test": "vue-cli-service test:unit",
    "test:coverage": "vue-cli-service test:unit --coverage"
  },
  "main": "dist/vcdonut.umd.js",
  "module": "dist/vcdonut.common.js",
  "unpkg": "dist/vcdonut.umd.min.js",
  "dependencies": {
    "core-js": "^3.6.5",
    "regenerator-runtime": "^0.13.5",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@vue/cli-plugin-babel": "^4.4.6",
    "@vue/cli-plugin-eslint": "^4.4.6",
    "@vue/cli-plugin-unit-jest": "^4.4.6",
    "@vue/cli-service": "^4.4.6",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "10.1.0",
    "babel-jest": "^26.1.0",
    "dev": "^0.1.3",
    "eslint": "^7.4.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-vue": "^6.2.2",
    "vue-jest": "^3.0.6",
    "vue-template-compiler": "^2.6.11"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "bugs": "https://github.com/dumptyd/vue-css-donut-chart/issues",
  "homepage": "https://dumptyd.github.io/vue-css-donut-chart/",
  "keywords": [
    "donut",
    "pie",
    "chart",
    "circle",
    "progress",
    "vue",
    "css",
    "pure CSS"
  ],
  "license": "MIT",
  "repository": "https://github.com/dumptyd/vue-css-donut-chart"
}
